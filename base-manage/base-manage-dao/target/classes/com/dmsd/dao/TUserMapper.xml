<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dmsd.dao.TUserMapper">

    <!--parameter是可以填基本数据类型的-->
    <select id="selectByPrimaryKey" parameterType="int" resultType="com.dmsd.pojo.TUser">
        SELECT * FROM USER WHERE id = #{id}
